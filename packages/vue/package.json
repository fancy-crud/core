{
  "name": "@fancy-crud/vue",
  "version": "0.0.0",
  "private": false,
  "author": {
    "name": "Christopher A. Flores",
    "url": "https://github.com/cafadev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fancy-crud/vue"
  },
  "main": "./dist/fancy-crud-vue.umd.js",
  "module": "./dist/fancy-crud-vue.mjs",
  "exports": {
    ".": {
      "import": "./dist/fancy-crud-vue.mjs",
      "require": "./dist/fancy-crud-vue.umd.js"
    },
    "./dist/fancy-crud-vue.css": {
      "import": "./dist/fancy-crud-vue.css",
      "require": "./dist/fancy-crud-vue.css"
    },
    "./src/styles/index.sass": {
      "import": "./src.styles/index.sass",
      "require": "./src.styles/index.sass"
    }
  },
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "build": "vite build"
  },
  "peerDependencies": {
    "@fancy-crud/core": "workspace:^",
    "@vueuse/core": "^9.9.0",
    "esno": "0.10.0",
    "lodash.isequal": "^4.5.0",
    "vue": "^3.3.2"
  },
  "dependencies": {
    "@fancy-crud/core": "workspace:^",
    "@vueuse/core": "^9.9.0",
    "esno": "0.10.0",
    "lodash.isequal": "^4.5.0",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3"
  },
  "release": {
    "branches": [
      "main",
      "next",
      "next-major",
      { "name": "beta", "prerelease": true },
      { "name": "alpha", "prerelease": true }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "./dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "form",
    "table",
    "datatable",
    "automation",
    "crud",
    "create",
    "read",
    "update",
    "delete",
    "vue"
  ]
}
