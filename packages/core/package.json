{
  "name": "@fancy-crud/core",
  "version": "0.0.0",
  "private": false,
  "author": {
    "name": "Christopher A. Flores",
    "url": "https://github.com/cafadev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fancy-crud/core.git"
  },
  "main": "dist/core.mjs",
  "module": "dist/core.mjs",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build"
  },
  "peerDependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next",
      "next-major",
      { "name": "beta", "prerelease": true },
      { "name": "alpha", "prerelease": true }
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      ["@semantic-release/exec", {
        "prepareCmd": "pnpm run -w packing ${nextRelease.version}",
        "publishCmd": "pnpm publish --no-git-checks",
        "successCmd": "echo ${nextRelease.version}"
      }],
      "@semantic-release/github"
    ]
  },
  "keywords": [
    "form",
    "table",
    "datatable",
    "automation",
    "crud",
    "create",
    "read",
    "update",
    "delete"
  ]
}
