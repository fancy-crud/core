{
  "packageManager": "pnpm@8.6.2",
  "private": true,
  "scripts": {
    "dev": "vite --port 9090 --host 0.0.0.0",
    "build": "vite build",
    "build-tsc": "tsc && tsc-alias --verbose -p ./tsconfig.json",
    "ci:publish": "node scripts/build.js && publish -r",
    "packing": "node scripts/build.js",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "demo"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@changesets/cli": "^2.26.2",
    "@mdi/font": "^7.2.96",
    "@oruga-ui/oruga-next": "^0.6.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/lodash": "^4.14.182",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.9.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.7",
    "happy-dom": "^10.3.2",
    "jsdom": "^20.0.0",
    "lodash.isequal": "^4.5.0",
    "nx": "^16.7.2",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "tailwindcss": "^3.2.4",
    "taze": "^0.8.2",
    "ts-node": "10.9.1",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-plugin-pages": "^0.26.0",
    "vitest": "^0.23.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.0.24",
    "zod": "^3.21.4"
  },
  "plugins": [
    "@semantic-release/changelog",
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "node scripts/build.js && pnpm run -w packing ${nextRelease.version} && pnpm --filter './packages/**' build && pnpm --filter './packages/**' pack",
        "publishCmd": "pnpm --filter './packages/**' publish --no-git-checks --tag ${branch.channel}",
        "successCmd": "echo '\n\n\n ${branch.name} Release completed ${nextRelease.version}'"
      }
    ]
  ],
  "release": {
    "branches": [
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
