{
  "packageManager": "pnpm@8.6.2",
  "private": true,
  "type": "module",
  "scripts": {
    "demo:oruga": "pnpm --filter='./demo-oruga' dev",
    "demo:vuetify": "pnpm --filter='./demo-vuetify' dev",
    "build:core": "pnpm --filter='./packages/core' build",
    "build:vue": "pnpm --filter='./packages/vue' build",
    "build:p-rule-parsers": "pnpm --filter='./packages/rules-processors' build",
    "build:p-vue3-toastify": "pnpm --filter='./packages/vue-toastify-plugin' build",
    "build:wo": "pnpm --filter='./packages/wrapper-oruga-ui' build",
    "build:wv": "pnpm --filter='./packages/wrapper-vuetify' build:no-ts",
    "ci:publish": "pnpm publish --filter './packages/**' --no-git-checks",
    "release": "node scripts/build.js",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "demo"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@changesets/cli": "^2.26.2",
    "@mdi/font": "^7.2.96",
    "@oruga-ui/oruga-next": "^0.6.0",
    "@oruga-ui/theme-bulma": "^0.2.11",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/lodash": "^4.14.182",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^10.4.1",
    "autoprefixer": "^10.4.13",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.7",
    "happy-dom": "^10.3.2",
    "jsdom": "^20.0.0",
    "lodash.isequal": "^4.5.0",
    "nx": "^16.7.2",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "semantic-release": "^19.0.0",
    "tailwindcss": "^3.3.4",
    "taze": "^0.8.2",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "valibot": "^0.19.0",
    "vite": "^4.5.0",
    "vitest": "^0.23.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.11",
    "vuetify": "^3.3.23",
    "zod": "^3.21.4"
  }
}
